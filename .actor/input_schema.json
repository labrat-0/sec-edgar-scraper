{
    "title": "AI/ML Intelligence Scraper Input",
    "description": "Search AI/ML models, research papers, and trending papers from HuggingFace and arXiv. No API key required.",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "mode": {
            "sectionCaption": "Scraping Mode",
            "sectionDescription": "Choose what type of AI/ML data to collect.",
            "title": "Scraping mode",
            "type": "string",
            "description": "What type of AI/ML data to search for.",
            "editor": "select",
            "enum": ["search_models", "search_papers", "trending_papers"],
            "enumTitles": [
                "Search Models (HuggingFace Hub)",
                "Search Papers (arXiv AI/ML)",
                "Trending Papers (HuggingFace Daily Papers)"
            ],
            "default": "search_models"
        },
        "query": {
            "sectionCaption": "Search Filters",
            "sectionDescription": "Filter models and papers by keyword, category, framework, and more.",
            "title": "Search query",
            "type": "string",
            "description": "Search keyword for models or papers. For arXiv, searches titles and abstracts.",
            "editor": "textfield",
            "prefill": "large language model"
        },
        "sort": {
            "title": "Sort by",
            "type": "string",
            "description": "How to sort results. Options depend on the mode.",
            "editor": "select",
            "enum": ["downloads", "likes", "trending", "relevance", "submittedDate", "lastUpdatedDate"],
            "enumTitles": [
                "Downloads (Models)",
                "Likes (Models)",
                "Trending (Models)",
                "Relevance (Papers)",
                "Submitted Date (Papers)",
                "Last Updated Date (Papers)"
            ],
            "default": "downloads"
        },
        "pipelineTag": {
            "title": "Pipeline task (Models only)",
            "type": "string",
            "description": "Filter models by pipeline task. Leave empty for all tasks.",
            "editor": "select",
            "enum": [
                "",
                "text-generation",
                "text-classification",
                "token-classification",
                "question-answering",
                "summarization",
                "translation",
                "fill-mask",
                "text2text-generation",
                "conversational",
                "text-to-image",
                "image-to-text",
                "image-classification",
                "object-detection",
                "image-segmentation",
                "automatic-speech-recognition",
                "text-to-speech",
                "audio-classification",
                "feature-extraction",
                "sentence-similarity",
                "zero-shot-classification",
                "reinforcement-learning",
                "depth-estimation",
                "image-to-image",
                "video-classification"
            ],
            "enumTitles": [
                "All Tasks",
                "Text Generation",
                "Text Classification",
                "Token Classification (NER)",
                "Question Answering",
                "Summarization",
                "Translation",
                "Fill-Mask",
                "Text-to-Text Generation",
                "Conversational",
                "Text-to-Image",
                "Image-to-Text",
                "Image Classification",
                "Object Detection",
                "Image Segmentation",
                "Speech Recognition (ASR)",
                "Text-to-Speech",
                "Audio Classification",
                "Feature Extraction",
                "Sentence Similarity",
                "Zero-Shot Classification",
                "Reinforcement Learning",
                "Depth Estimation",
                "Image-to-Image",
                "Video Classification"
            ],
            "default": ""
        },
        "libraryFilter": {
            "title": "Framework / Library (Models only)",
            "type": "string",
            "description": "Filter models by ML framework. Leave empty for all frameworks.",
            "editor": "select",
            "enum": [
                "",
                "transformers",
                "diffusers",
                "pytorch",
                "tensorflow",
                "jax",
                "onnx",
                "safetensors",
                "gguf",
                "spacy",
                "keras",
                "sklearn",
                "sentence-transformers",
                "peft",
                "adapter-transformers"
            ],
            "enumTitles": [
                "All Frameworks",
                "Transformers",
                "Diffusers",
                "PyTorch",
                "TensorFlow",
                "JAX",
                "ONNX",
                "Safetensors",
                "GGUF",
                "spaCy",
                "Keras",
                "scikit-learn",
                "Sentence Transformers",
                "PEFT",
                "Adapter Transformers"
            ],
            "default": ""
        },
        "arxivCategory": {
            "sectionCaption": "arXiv Filters (Papers only)",
            "sectionDescription": "Filters specific to arXiv paper search.",
            "title": "arXiv category",
            "type": "string",
            "description": "Filter papers by arXiv category. Leave empty to search all AI/ML categories.",
            "editor": "select",
            "enum": [
                "",
                "cs.AI",
                "cs.LG",
                "cs.CL",
                "cs.CV",
                "cs.NE",
                "cs.RO",
                "cs.IR",
                "cs.MA",
                "stat.ML",
                "cs.SD",
                "eess.AS",
                "cs.HC",
                "cs.CR"
            ],
            "enumTitles": [
                "All AI/ML Categories",
                "cs.AI - Artificial Intelligence",
                "cs.LG - Machine Learning",
                "cs.CL - Computation and Language (NLP)",
                "cs.CV - Computer Vision",
                "cs.NE - Neural and Evolutionary Computing",
                "cs.RO - Robotics",
                "cs.IR - Information Retrieval",
                "cs.MA - Multi-Agent Systems",
                "stat.ML - Machine Learning (Statistics)",
                "cs.SD - Sound",
                "eess.AS - Audio and Speech Processing",
                "cs.HC - Human-Computer Interaction",
                "cs.CR - Cryptography and Security (AI Safety)"
            ],
            "default": ""
        },
        "author": {
            "title": "Author name (Papers only)",
            "type": "string",
            "description": "Filter arXiv papers by author name.",
            "editor": "textfield"
        },
        "dateFrom": {
            "title": "Date from (Papers only)",
            "type": "string",
            "description": "Filter papers published from this date (YYYY-MM-DD).",
            "editor": "textfield"
        },
        "dateTo": {
            "title": "Date to (Papers only)",
            "type": "string",
            "description": "Filter papers published up to this date (YYYY-MM-DD).",
            "editor": "textfield"
        },
        "maxResults": {
            "sectionCaption": "Scraper Settings",
            "sectionDescription": "Control output limits.",
            "title": "Max results",
            "type": "integer",
            "description": "Maximum number of results to return. Free users are limited to 25 results per run.",
            "default": 100,
            "minimum": 1,
            "maximum": 10000
        }
    },
    "required": ["mode"]
}
